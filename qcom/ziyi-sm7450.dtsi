    };

    pmk8350_xo_therm {
        reg = <PMK8350_ADC7_AMUX_THM1_100K_PU>;
        label = "pmk8350_xo_therm";
        qcom,ratiometric;
        qcom,hw-settle-time = <200>;
        qcom,pre-scaling = <1 1>;
    };

    pmk8350_sc8551_therm {
        reg = <PMK8350_ADC7_AMUX_THM2_100K_PU>;
        label = "pmk8350_sc8551_therm";
        qcom,ratiometric;
