&qupv3_se11_spi {
	status = "ok";
	ir-spi@0 {
		compatible = "ir-spi";
		reg = <0>;
		spi-max-frequency = <19200000>;
		status = "ok";
		};
};

&qupv3_se3_i2c {
    status = "ok";
	aw2033@45 {
		compatible = "awinic,aw2033_led";
		reg = <0x45>;
		status = "okay";
		aw2033,red {
			aw2033,name = "red";
			aw2033,id = <0>;
			aw2033,imax = <2>;
			aw2033,led-current = <3>;
			aw2033,max-brightness = <255>;
			aw2033,rise-time-ms = <6>;
			aw2033,hold-time-ms = <0>;
			aw2033,fall-time-ms = <6>;
			aw2033,off-time-ms = <4>;
		};

		aw2033,green {
			aw2033,name = "green";
			aw2033,id = <1>;
			aw2033,imax = <2>;
			aw2033,led-current = <3>;
			aw2033,max-brightness = <255>;
			aw2033,rise-time-ms = <6>;
			aw2033,hold-time-ms = <0>;
			aw2033,fall-time-ms = <6>;
			aw2033,off-time-ms = <4>;
		};
		aw2033,blue {
			aw2033,name = "blue";
			aw2033,id = <2>;
			aw2033,imax = <2>;
			aw2033,led-current = <3>;
			aw2033,max-brightness = <255>;
			aw2033,rise-time-ms = <6>;
			aw2033,hold-time-ms = <0>;
			aw2033,fall-time-ms = <6>;
			aw2033,off-time-ms = <4>;
		};
	};
};