#include "mayfly-pinctrl.dtsi"
#include "xiaomi-sm8475-common.dtsi"

&qupv3_se15_i2c {
	status = "ok";

	/* TOP Smart PA */
	aw882xx_smartpa@34 {
		compatible = "awinic,aw882xx_smartpa";
		reg = <0x34>;
		irq-gpio = <&tlmm 63 0x2008>;
		dc-flag = <0>;
		sound-channel = <0>;
		aw-cali-mode = "aw_none ";
		status = "ok";
	};

	/* BOT Smart PA */
	aw882xx_smartpa@35 {
		compatible = "awinic,aw882xx_smartpa";
		reg = <0x35>;
		irq-gpio = <&tlmm 118 0x2008>;
		dc-flag = <0>;
		sound-channel = <1>;
		aw-cali-mode = "aw_none ";
		status = "ok";
	};
};
